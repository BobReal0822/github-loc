{
    "manifest_version": 2,
  
    "name": "Githib Loc",
    "description": "Counts the lines of code in a github repository.",
    "version": "1.0",
  
    "author": "ephoton",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "index.html",
      "default_title": "Github loc"
    },
    "permissions": [
      "activeTab",
      "storage",
      "background",
      "debugger"
    ]
  }
  